fn main(args: String[1]) {
    const inputStr = args[0];
    const parsingResult = parse_int(inputStr).expect("Cannot parse \n $inputStr: $parsingResult");

    println("fibo($input) = ${fibo(input)}");
}

fn fibo(n: int): int {
    if (n <= 1) {
        return n;
    }
    return fibo(n - 1) + fibo(n - 2);
}